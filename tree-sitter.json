{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "authzed",
      "camelcase": "AuthZed",
      "title": "authzed-parser",
      "scope": "source.authzed",
      "file-types": [
        "authzed"
      ],
      "injection-regex": "^authzed$",
      "class-name": "TreeSitterAuthzed"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Authzed grammar for tree-sitter",
    "authors": [
      {
        "name": "Ani Amer",
        "email": "m.elsayedamer@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-authzed"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}